{
	"extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
	"rules": {
		"no-console": "warn",
		"no-undef": 0,
		"no-unused-vars": 0
	},
	"parser": "babel-eslint",
	"plugins": ["import"],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
